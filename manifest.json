{
  "manifest_version": 2,

  "name": "Crater",
  "description": "Live Track Cryptocurrency prices without opening extra tab",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Crater"
  },
  "permissions": [
    "https://koinex.in/api/ticker"
  ]
}